from typing import Any, ClassVar

class PeriodicTable:
    data: ClassVar[list[list[Any]]]
    symtonum: ClassVar[dict[str, int]]
    dummysymbols: ClassVar[list[str]]
    @classmethod
    def get_atomic_number(cls, symbol: str) -> int: ...
    @classmethod
    def get_symbol(cls, atnum: int) -> str: ...
    @classmethod
    def get_mass(cls, arg: str | int) -> float: ...
    @classmethod
    def get_radius(cls, arg: str | int) -> float: ...
    @classmethod
    def get_connectors(cls, arg: str | int) -> int: ...
    @classmethod
    def get_metallic(cls, arg: str | int) -> int: ...
    @classmethod
    def get_electronegative(cls, arg: str | int) -> int: ...
    @classmethod
    def set_mass(cls, element: str, value: float) -> None: ...
    @classmethod
    def set_radius(cls, element: str, value: float) -> None: ...
    @classmethod
    def set_connectors(cls, element: str, value: float) -> None: ...

PT = PeriodicTable
